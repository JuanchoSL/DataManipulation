    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/JuanchoSL/DataManipulation",
    "keywords": [
        "variables",
        "manipulation",
        "strings",
        "numbers",
        "arrays"
    ],
    "authors": [
        {
            "name": "Juan SÃ¡nchez Lecegui",
            "email": "Juan.Sanchez@tecnicosweb.com",
